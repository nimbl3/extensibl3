{
  "name": "Extensibl3",
  "version": "0.1",
  "description": "Improve Nimbl3 Development Workflow!",
  "permissions": ["storage"],
  "manifest_version": 2,
  "icons": {
    "16": "public/icons/icon_16.png",
    "48": "public/icons/icon_48.png",
    "128": "public/icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["build/index.js"],
      "run_at": "document_start"
    }
  ]
}